<div>
  {{#with main}}
  <main>
    <section>
      {{> title data='My Education' big=true}}
      <ul>
        {{#each educations}}
          <li>
            {{name}}:  
            {{#unless isEnd}}
              Ще в процесі
            {{/unless}}
          </li>
        {{/each}}
      </ul>
    </section>

    <section>
      {{> title data='My Certificates'}}
      <ul>
        {{#each certificates}}
          <li>
            {{name}} : ({{id}})
          </li>
        {{/each}}
      </ul>
    </section>
  </main>
  {{/with}}
</div>

